[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = Yusaira Khan
	email = yusairak@nulogy.com
[push]
	default = simple
[remote "origin"]
	push = HEAD
[branch]
	autosetupmerge = true
[color]
	branch = auto
	diff = auto
	ui = true
[core]
	commentchar = $
[fetch]
	prune = true
[alias]
	br = branch
	co = "commit -v"
	cm = "commit -v"
	ch = checkout
	cho = checkout
	chb = "checkout -b"
	st = status
	branch-name = "!git rev-parse --abbrev-ref HEAD"
	pnew = "!git push -u origin $(git branch-name)"
	rhh = "reset --hard"
	rb = "rebase -i"
	pl = "pull -- rebase"
	a = add
	ah = "add ."
	cp = cherry-pick
	sh = show
	rf = reflog






